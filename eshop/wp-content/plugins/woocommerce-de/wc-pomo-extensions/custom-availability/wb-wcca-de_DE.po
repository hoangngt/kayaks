#: woocommerce-custom-availability.php:180
msgid "Default"
msgstr "Standard"

#@ wb-wcca
#: woocommerce-custom-availability.php:128
msgid "Low Stock Font Colour"
msgstr "'Niedrige Lagermenge' Schriftfarbe"

#@ wb-wcca
#: woocommerce-custom-availability.php:135
msgid "Low Stock Font Weight"
msgstr "'Niedrige Lagermenge' Schriftgewicht"

#@ wb-wcca
#: woocommerce-custom-availability.php:147
msgid "Out of Stock Font Colour"
msgstr "'Vergriffen' Schriftfarbe"

#@ wb-wcca
