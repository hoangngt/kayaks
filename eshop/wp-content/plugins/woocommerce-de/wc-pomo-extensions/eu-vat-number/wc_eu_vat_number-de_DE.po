#@ wc_eu_vat_number
#: eu-vat-number.php:63
msgid "EU VAT Title"
msgstr "EU Ust.-ID-Nr. Titel"

#@ wc_eu_vat_number
#: eu-vat-number.php:64
msgid "The title that appears at checkout above the VAT Number box."
msgstr "Der Titel erscheint während des Bezahlvorgangs über dem Ust.-ID-Nr. Eingabefeld."

#@ wc_eu_vat_number
#: eu-vat-number.php:69
msgid "EU VAT Message"
msgstr "EU Ust.-ID-Nr. Mitteilung"

#@ wc_eu_vat_number
#: eu-vat-number.php:70
msgid "The message that appears at checkout above the VAT Number box."
msgstr "Die Mitteilung erscheint während des Bezahlvorgangs über dem Ust.-ID-Nr. Eingabefeld."

#@ wc_eu_vat_number
#: eu-vat-number.php:76
msgid "Show field for base country"
msgstr "Feld für Basisland anzeigen"

#@ wc_eu_vat_number
#: eu-vat-number.php:77
msgid "Show the VAT field even when the customer is in your base country"
msgstr "Das Ust.-ID-Nr. Feld auch dann anzeigen, wenn der Kunde aus Ihrem Basisland ist"

#@ wc_eu_vat_number
#: eu-vat-number.php:82
msgid "Deduct VAT for base country"
msgstr "Vorsteuerabzug für das Basisland"

#@ wc_eu_vat_number
#: eu-vat-number.php:83
msgid "Deduct the VAT even when the customer is in your base country and has a valid number"
msgstr "Vorsteuerabzug auch dann, wenn der Kunde aus Ihrem Basisland ist und eine gültige Ust,-ID-Nr. eingegeben hat"

#@ wc_eu_vat_number
#: eu-vat-number.php:90
msgid "EU VAT Number"
msgstr "EU Ust.-ID-Nummer"

#@ wc_eu_vat_number
#: eu-vat-number.php:91
msgid "If you are in the EU and have a valid EU VAT registration number please enter it below."
msgstr "Wenn Sie wohnhaft in der EU sind und eine gültige EU Ust.-ID-Nummer besitzen, geben Sie diese bitte hier ein."

#@ wc_eu_vat_number
#: eu-vat-number.php:153
#: eu-vat-number.php:154
msgid "VAT Number"
msgstr "Ust.-ID-Nummer"

#@ wc_eu_vat_number
#: eu-vat-number.php:283
#, php-format
msgid "You cannot use a VAT number since your country (%s) is outside of the EU."
msgstr "Sie können keine Ust.-ID-Nummer verwenden, da Ihr Land (%s) sich außerhalb der EU befindet."

#@ wc_eu_vat_number
#: eu-vat-number.php:321
#, php-format
msgid "You have entered an invalid VAT number (%s) for your country."
msgstr "Sie haben eine ungültige Ust.-ID-Nummer (%s) für Ihr Land eingegeben."

