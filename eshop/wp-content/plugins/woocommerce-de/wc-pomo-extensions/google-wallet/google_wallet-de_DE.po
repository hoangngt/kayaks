#, php-format
msgid "Subscription for \"%s\""
msgstr "Abonnement für \"%s\""

#@ google_wallet
#: gateway-google_checkout.php:780
#, php-format
msgid "(Order %s)"
msgstr "(Bestellung %s)"

#@ google_wallet
#: gateway-google_checkout.php:783
