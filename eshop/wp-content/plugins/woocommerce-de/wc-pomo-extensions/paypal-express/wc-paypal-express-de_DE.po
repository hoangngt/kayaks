msgstr "\"Checkout with PayPal\" Bild-Button verwenden (Bezahlen mit PayPal)"

#@ wc-paypal-express
#: woocommerce-gateway-paypal-express.php:300
msgid "Standard checkout button"
msgstr "Standard-Bezahlen-Button"

#@ wc-paypal-express
#: woocommerce-gateway-paypal-express.php:302
msgid "Hide standard checkout button on cart page"
msgstr "Standard-Bezahlen-Button auf der Warenkorb-Seite ausblenden"

#@ wc-paypal-express
#: woocommerce-gateway-paypal-express.php:306
msgid "Standard checkout"
msgstr "Standard-Bezahlen/ -Kasse"

#@ wc-paypal-express
#: woocommerce-gateway-paypal-express.php:308
msgid "Show express checkout button on checkout page"
msgstr "Express-Bezahlen-Button auf der Kasse-Seite anzeigen"

#@ wc-paypal-express
#: woocommerce-gateway-paypal-express.php:312
msgid "PayPal Account Optional"
msgstr "PayPal Konto optional"

#@ wc-paypal-express
#: woocommerce-gateway-paypal-express.php:314
msgid "Allow customers to checkout without a PayPal account using their credit card. \"PayPal Account Optional\" must be turned on in your PayPal account. "
msgstr "Ermöglichen Sie es Ihren Kunden über einem PayPal-Konto zu bezahlen, indem diese ihre Kreditkarte verwenden können. \"PayPal Konto optional\" (PayPal Account Optional) muss angeschaltet sein in Ihrem PayPal-Benutzerkonto. "

#@ wc-paypal-express
